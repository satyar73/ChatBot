# Company configuration
COMPANY_NAME=SVV

# API Keys
OPENAI_API_KEY=your_openai_key
PINECONE_API_KEY=your_pinecone_key
PINECONE_ENVIRONMENT=your_pinecone_env
PINECONE_INDEX=your_index_name
SHOPIFY_API_KEY=your_shopify_key
GOOGLE_API_KEY=your_google_api_key
SHOPIFY_STORE="your store id"
SHOPIFY_SHOP_DOMAIN="store-id.myshopify.com"
SHOPIFY_SITE_BASE_URL="your url"

# Portkey Configuration (if using Portkey)
PORTKEY_API_KEY="your_portkey_key"
PORTKEY_VIRTUAL_KEY="virtual-key"
PORTKEY_CONFIG="config-file"
PORTKEY_CACHE_TTL=3600
PORTKEY_CACHE_STRATEGY=semantic
PORTKEY_CACHE_SIMILARITY=0.95

# Environment Settings
# production, development, testing
ENVIRONMENT=production
# DEBUG, INFO, WARNING, ERROR
LOG_LEVEL=INFO

# Cache Settings
CACHE_ENABLED=true
 # 24 hours in seconds
CACHE_TTL=86400
MAX_CACHE_ENTRIES=1000
